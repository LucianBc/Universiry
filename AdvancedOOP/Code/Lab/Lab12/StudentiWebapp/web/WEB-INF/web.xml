<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>studenti</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>StudentiList</servlet-name>
        <servlet-class>com.lucianbc.pao.studenti.StudentiList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentPage</servlet-name>
        <servlet-class>com.lucianbc.pao.studenti.StudentPage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentiList</servlet-name>
        <url-pattern>/studenti</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentPage</servlet-name>
        <url-pattern>/student</url-pattern>
    </servlet-mapping>
</web-app>